{
  "name": "gulp-preset",
  "version": "1.1.0-alpha.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "gulp test",
    "build": "cross-env NODE_ENV=production npm run builtin:build",
    "beta": "cross-env NODE_ENV=development FUCK=true npm run builtin:build",
    "watch": "cross-env NODE_ENV=development gulp watch",
    "git:commit": "git add .&&git cz",
    "git:version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md && git cz",
    "git:alpha": "standard-version --prerelease alpha -m 'alpha内测版本'",
    "git:beta": "standard-version --prerelease beta -m 'beta公测版本'",
    "git:release": "standard-version --release-as minor --tag-prefix 'stable-' -m 'stable稳定版本'",
    "builtin:build": "gulp webpack && gulp && npm run builtin:rev && gulp cleanWebpackTemp",
    "builtin:rev": "gulp rev",
    "builtin:webpack": "gulp webpack",
    "builtin:cleanWebpackTemp": "gulp cleanWebpackTemp"
  },
